/* compoents/weekCalendar/weekCalendar.wxss */
.row { 
  display:flex; 
  flex-direction:row;  
  justify-content: space-around; 
  align-items: center;
  background-color:#F6F6F6;  
}
/* .tableHead{
  padding-left: 29.5px;
} */
 
.column{
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  flex: 1;
  margin: 5rpx 10rpx;
}

.calender-header {
  color: #59518d;
  font-size: large;
} 

.tableHead {
  color: #59518d;
  height: 150rpx;
}

.select-column{
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  flex: 1;
  border-radius: 50%;
  color: #fff;
  background: #0090F0;
}
 
.timeType{
  font-size: 20px;
  font-weight:bold;
}
 
.dateType{
  margin: 3rpx;
  font-size: 15px;
  font-weight:bold;
}
 
.weekType{
  margin: 3rpx;
  font-size: 15px;
  color: rgb(212,212,212);
}
 
.line{
  height: 1rpx;
  width: 100%;
  background-color: gainsboro;
}
 
.line-shadow{
  height: 3rpx;
  width: 100%;
  background-color: gainsboro;
  box-shadow:5px 0px 5px 0px gainsboro;
}

